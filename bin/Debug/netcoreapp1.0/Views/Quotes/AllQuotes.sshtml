<html>
<head>
	<title>All Quotes</title>
	<link rel="stylesheet" href="Content/style.css"/>
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.css" >
</head>
<body>
	<h2>Here are some of the awesome quotes</h2>
    @Each 
        <p>@Current.quote</p>
        <p>-@Current.user at @Current.created_at</p>
        <hr>
        @EndEach
    <a href='/'><button>Add Quote</button></a>
</body>
</html>
